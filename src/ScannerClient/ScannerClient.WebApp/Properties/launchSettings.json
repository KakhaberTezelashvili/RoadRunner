{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:61621",
      "sslPort": 44378
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "commandLineArgs": "--pathbase=/scanner-client",
      "launchBrowser": true,
      "launchUrl": "https://localhost:44378/scanner-client",
      "inspectUri": "{wsProtocol}://{url.hostname}:{url.port}/scanner-client/_framework/debug/ws-proxy?browser={browserInspectUri}",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "ScannerClient.WebApp": {
      "commandName": "Project",
      "commandLineArgs": "--pathbase=/scanner-client",
      "launchBrowser": true,
      "launchUrl": "https://localhost:5201/scanner-client",
      "inspectUri": "{wsProtocol}://{url.hostname}:{url.port}/scanner-client/_framework/debug/ws-proxy?browser={browserInspectUri}",
      "applicationUrl": "https://localhost:5201;http://localhost:5200",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Docker": {
      "commandName": "Docker",
      "commandLineArgs": "--pathbase=/scanner-client",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/scanner-client",
      "publishAllPorts": true,
      "useSSL": true,
      "httpPort": 5200,
      "sslPort": 5201
    }
  }
}
